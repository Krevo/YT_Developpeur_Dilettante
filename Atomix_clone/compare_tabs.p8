function equal(ta, tb)
	if #ta ~= #tb then return false end
	for i,v in ipairs(ta) do
		if #ta[i] ~= #tb[i] then return false end
		for j,w in ipairs(ta[i]) do
			--print (ta[i][j].." = ".. tb[i][j].." ?")
			if ta[i][j] ~= tb[i][j] then return false end
		end		
	end
	return true
end

function bool2txt(b)
	if b then
		return "oui"
	else
		return "non"
	end
end

print("t1 = tcible ? "..bool2txt(equal(t1, tcible)))
print("t2 = tcible ? "..bool2txt(equal(t2, tcible)))
print("t3 = tcible ? "..bool2txt(equal(t3, tcible)))
print("t4 = tcible ? "..bool2txt(equal(t4, tcible)))
     
